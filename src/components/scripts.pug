script.
	(function () {
	if ($(window).width() >= 768) {
	var bg = '#{global.bg}'.split(',')
	window.$("#bg").backstretch(bg, {
	duration: 1e4,
	alignY: 0,
	transition: "fade",
	transitionDuration: 1e3,
		start:Math.floor(Math.random() * bg.length)
	})
	}
	})()
